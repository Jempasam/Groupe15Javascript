{
    "deno.enable": false,
    "lit-html.tags": [
        "html",
        "raw",
        "dom"
    ],
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "typescript.preferences.importModuleSpecifierEnding": "js",
    "hidefiles.configurationType": "global",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    }
}